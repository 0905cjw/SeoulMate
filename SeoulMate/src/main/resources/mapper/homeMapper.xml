<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="com.seoulmate.home.dao.HomeDAO">
  	<select id="getHouseMap" resultType="String">
  		select addr from housewrite
	</select>
  	<select id="getMateMap" resultType="String">
  		select area from matewrite where matestate='모집중'
	</select>
 	<select id="getPreHouse" resultType="com.seoulmate.home.vo.HouseRoomVO">
  		<![CDATA[select * from housewrite where rownum <=3 order by writedate desc]]>
	</select>
	
	
  </mapper>